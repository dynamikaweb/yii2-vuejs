{
    "name": "dynamikaweb/yii2-vuejs",
    "description": "dependency to use Vue progressive JavaScript Framework with Yii2.",
    "type": "yii2-extension",
    "keywords": ["dynamika", "yii2", "vuejs"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Rodrigo Dornelles",
            "email": "rodrigo@dynamika.com.br",
            "role": "Developer"
        },
        {
            "name": "Robson Soares",
            "email": "robson@dynamika.com.br",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "http://github.com/dynamikaweb/yii2-vuejs/issues",
        "source": "http://github.com/dynamikaweb/yii2-vuejs"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "*",
        "npm-asset/vue": "2.*",
        "npm-asset/vue--cli-service": "*"
    },
    "autoload": {
        "psr-4": {
            "dynamikaweb\\vuejs\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
